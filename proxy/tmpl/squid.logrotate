/var/log/squid/access.log {
    weekly
    rotate 52
    compress
    notifempty
    missingok
    postrotate
    /etc/init.d/rsyslog reload
    endscript
}
/var/log/squid/cache.log {
    weekly
    rotate 10
    compress
    notifempty
    missingok
    postrotate
    /usr/share/eole/squid_logrotate.sh
    endscript
}
